<!-- <% const nama = 'Kawan kawan'; %> -->
<h1>SELAMAT DATANG MAHASISWA</h1>
<h3>Hallo <%= nama %></h3>

<h4>Daftar Mahasiswa</h4>
<% if (mahasiswa.length < 1) { %>
    <p>Data Masih Kosong</p>
<% } %>

<% mahasiswa.forEach(mhs => { %>
    <ul>
        <li>Nama : <%= mhs.nama %></li>
        <li>Email : <%= mhs.email %></li>
    </ul>
<% }) %>
